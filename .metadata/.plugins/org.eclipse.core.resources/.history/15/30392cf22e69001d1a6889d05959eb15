#include "ai.h"


void ai_mover(int count, WORD keycode) {
//	if (count % 100000
	if (keycode == 14) {
		if (count < 10) {
			vga_ctrl->VRAM[0] = 19;
		} else if (count < 20) {
			vga_ctrl->VRAM[0] = 26;
		} else if (count < 30) {
			vga_ctrl->VRAM[0] = 22;
		} else if (count < 40) {
			vga_ctrl->VRAM[0] = 07;
		}
	} else {
		vga_ctrl->VRAM[0] = 0;
	}
}
